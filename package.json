{
  "name": "generic",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://yasintz.github.io/cdn",
  "scripts": {
    "dev": "vite",
    "build:projects": "mkdir dist/projects && cp -r projects/* dist/projects",
    "build:sudoku": "mkdir dist/sudoku && node sudoku/build.js",
    "build:wordle": "mkdir dist/wordle && cd wordle && webpack",
    "build:vite": "tsc && vite build && cp build/vite/index.html build/vite/404.html && cp -r build/vite/* dist",
    "build": "(rm -rf ./dist||true) && mkdir dist && pnpm build:projects && pnpm build:sudoku && pnpm build:wordle && pnpm build:vite"
  },
