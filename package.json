    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-router-dom": "^6.6.2",
    "styled-components": "^5.3.6"
  },
